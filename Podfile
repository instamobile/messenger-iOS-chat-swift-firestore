# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'ChatApp' do
  use_frameworks!

  # Pods for ChatApp
  pod 'MessageKit'
  pod 'FBSDKLoginKit'

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'

  pod 'Alamofire'
  pod 'Kingfisher'

end